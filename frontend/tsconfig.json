{
  "compilerOptions": {
    "module": "Node16",
    "target": "ES2020",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "typeRoots": ["./node_modules/@types", "./types", "./src/types"],
    "types": ["node", "react", "react-dom", "react-router-dom"],
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@shared/*": ["../shared/*"]
    }
  },
  "include": [
    "src",
    "types",
    "../archive/RegisterLink.tsx",
    "../archive/InputForm.tsx"
  ],
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ]
}
